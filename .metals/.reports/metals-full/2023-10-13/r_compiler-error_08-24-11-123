package deMux

import chisel3._
import chisel3.util._

class deMux() extends Module { //1to2 
    //for multibit result(and in) could use bundle of bools to represent bits????
    val io = IO(new Bundle {
        val sel        = Input(Bool())
        val in         = Input(UInt(3.W))
        val d1         = Output(UInt(3.W))
        val d2         = Output(UInt(3.W))
    })

    val notSel = ~io.sel

    for(@@)

    val d1b1 = notSel & io.in{0}
    val d1b2 = notSel & io.in{1}
    val d1b3 = notSel & io.in{2}

    io.d1 := Cat(d1b3, d1b2, d1b1)


    val d2b1 = io.sel & io.in{0}
    val d2b2 = io.sel & io.in{1}
    val d2b3 = io.sel & io.in{2}

    io.d2 := Cat(d2b3, d2b2, d2b1)

    // io.d1 := notSel & io.in
    // io.d2 := io.sel & io.in
}

